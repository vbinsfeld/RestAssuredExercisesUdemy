{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "name",
    "releaseDate",
    "reviewScore",
    "category",
    "rating"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Resident Evil 4"
      ],
      "pattern": "^(.*)$"
    },
    "releaseDate": {
      "$id": "#/properties/releaseDate",
      "type": "string",
      "title": "The Releasedate Schema",
      "default": "",
      "examples": [
        "2005-10-01"
      ],
      "pattern": "^(.*)$"
    },
    "reviewScore": {
      "$id": "#/properties/reviewScore",
      "type": "integer",
      "title": "The Reviewscore Schema",
      "default": 0,
      "examples": [
        85
      ]
    },
    "category": {
      "$id": "#/properties/category",
      "type": "string",
      "title": "The Category Schema",
      "default": "",
      "examples": [
        "Shooter"
      ],
      "pattern": "^(.*)$"
    },
    "rating": {
      "$id": "#/properties/rating",
      "type": "string",
      "title": "The Rating Schema",
      "default": "",
      "examples": [
        "Universal"
      ],
      "pattern": "^(.*)$"
    }
  }
}